<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ws.freeboard" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="networkinterface">
        <param name="android-package" value="com.albahra.plugin.networkinterface.networkinterface" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="FileChooser">
        <param name="android-package" value="com.cesidiodibenedetto.filechooser.FileChooser" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="VideoPlugin">
        <param name="android-package" value="com.knowlounge.plugins.VideoPlugin" />
    </feature>
    <feature name="PollPlugin">
        <param name="android-package" value="com.knowlounge.plugins.PollPlugin" />
    </feature>
    <feature name="RoomPlugin">
        <param name="android-package" value="com.knowlounge.plugins.RoomPlugin" />
    </feature>
    <feature name="MultiPagePlugin">
        <param name="android-package" value="com.knowlounge.plugins.MultiPagePlugin" />
    </feature>
    <feature name="UserPlugin">
        <param name="android-package" value="com.knowlounge.plugins.UserPlugin" />
    </feature>
    <feature name="CommunicationPlugin">
        <param name="android-package" value="com.knowlounge.plugins.CommunicationPlugin" />
    </feature>
    <feature name="CommonPlugin">
        <param name="android-package" value="com.knowlounge.plugins.CommonPlugin" />
    </feature>

    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <allow-intent href="market:*" />
    <name>fbwebapp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-navigation href="blob:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="showTitle" value="false" />
    <preference name="fullscreen" value="false" />

</widget>
